{
  "name": "@overlastic/svelte",
  "type": "module",
  "version": "0.8.1",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/overlastic#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/overlastic.git"
  },
  "bugs": {
    "url": "https://github.com/hairyf/overlastic/issues"
  },
  "keywords": [
    "unified",
